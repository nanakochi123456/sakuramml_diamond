Function VVoice(v) {
	IF(VOCALOID=0) {
		IF(SMAFMODE=3) {
			Voice(v,124,2)
		} ELSE { IF(SMAFMODE=7) {
				Voice(v,124,2)
			} ELSE {
				Voice(v)
			}
		}
	}
}

Function Ex(v1,v2,t) {
	IF(VV=1) {
//		IF(SMAFMODE=0) {
			Expression.onTime(v1,v2,t)
//		}
	}
}

Function LoopPlay(Str Mml, Str Add) {
	Str _MML
	_MML=_MML + #STR(Mml)
	IF(BGMONLY=0) {
		LOOP=2
	}
	IF(SMAFMODE<>0) {
		LOOP=1
	}
	For (Int i = 1; i < LOOP; i++) {
		_MML=_MML + #STR(Add) + #STR(Mml)
	}
	IF(SMAFMODE<>0) {
		_MML=_MML + #STR(Add)
	}
	_MML
}

// Lyric define
Function L(Str s) {
	IF(LYRICDISPLAY=1) {
		IF(K=1) {
			Lyric=s
		}
	}
}

